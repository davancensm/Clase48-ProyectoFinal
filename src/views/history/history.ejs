<h1>Productos disponibles</h1>
<% if (products.length>0) { %>
    <% products.forEach(product => { %>
        <div>
            <table>
                <tr>
                <td>
                    <p>
                        <%=product.name  %>
                    </p>
                    <p>
                        <%= product.model  %>
                    </p>
                    <p>
                        <%=product.bando  %>
                    </p>
                    <p>
                        <%=product.description  %>
                    </p>
                </td>
                <td>
                    <img src=<%=product.url %>>
                </td>
                <td>
                    <p>
                        <%=product.price  %>
                    </p>
                    <p>
                        <%=product.stock  %>
                    </p>
                </td>    
                </tr>
            </table>
        </div>
        <% }) %>
            <% } else { %>
                <p>Por ahora, no hay informacion disponible</p>
                <% } %>